
@model Apps.Models.sys.SysSampleModel
@using Apps.Common;
@using Apps.Models.sys;
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Index_LayoutEdit.cshtml";

}
<script type="text/javascript">
    $(function () {
        $("#btnReturn").click(function () {
            window.parent.frameReturnByClose();
        });
    });
</script>
<div class="mvctool bgb">
    <a id="btnReturn" style="float: left;" class="l-btn l-btn-plain"><span class="l-btn-left"><span class="l-btn-text icon-return" style="padding-left: 20px;">返回</span></span></a>
</div>
@using (Html.BeginForm())
{
    <table class="form_table setinput355">
        <tbody>
            <tr>
                <th>
                    @Html.LabelFor(model => model.Id)
                </th>
                <td>
                    @Html.EditorFor(model => model.Id)
                </td>
            </tr>
            <tr>
                <th>
                    @Html.LabelFor(model => model.Name)
                </th>
                <td>
                    @Html.TextAreaFor(model => model.Name)
                </td>
            </tr>
            <tr>
                <th>
                    @Html.LabelFor(model => model.Age)
                </th>
                <td>
                    @Html.EditorFor(model => model.Age)
                </td>
            </tr>
            <tr>
                <th>
                    @Html.LabelFor(model => model.Bir)
                </th>
                <td>
                    @Html.EditorFor(model => model.Bir)
                </td>
            </tr>
            <tr>
                <th>
                    @Html.LabelFor(model => model.Note)
                </th>
                <td>
                    @Html.EditorFor(model => model.Note)
                </td>
            </tr>
            <tr>
                <th>
                    @Html.LabelFor(model => model.CreateTime)
                </th>
                <td>
                    @Html.TextBoxFor(model => model.CreateTime)
                </td>
            </tr>
            <tr>
                <th>
                    @Html.LabelFor(model => model.Photo)
                </th>
                <td>
                    @Html.TextBoxFor(model => model.Photo)
                </td>
            </tr>
        </tbody>
    </table>
}
